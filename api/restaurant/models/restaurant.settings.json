{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "name": "Restaurant"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "rating": {
      "type": "float"
    },
    "logo": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "menus": {
      "via": "restaurant",
      "collection": "menu"
    },
    "owners": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "restaurants",
      "dominant": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "favorites"
    }
  }
}
